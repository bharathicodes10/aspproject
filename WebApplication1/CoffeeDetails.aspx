<%@ Page Title="" Language="C#" MasterPageFile="~/Site1.Master" AutoEventWireup="true" CodeBehind="CoffeeDetails.aspx.cs" Inherits="WebApplication1.CoffeeDetails" %>

<asp:Content ID="Content4" ContentPlaceHolderID="coffeeDetails" runat="server">

	<div class="row">
		<div class="column">
			<img src="data:image/jpeg;base64," alt="Coffee 2" style="width:100%">
		</div>
		<div class="column">
			<h2>Robusta</h2>
			<p>Indian Rupees 200</p>
			<p>Robusta coffee is a species of coffee that is known for its strong, earthy flavor and high caffeine content. It is often used in blends with other coffee varieties to add body and depth to the flavor.</p>
			<button onclick="confirmOrder('Robusta', 200)">Order</button>
		</div>
	</div>

	<style>
		
    .row {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
    }

    .column {
        flex-basis: calc(33.33% - 8px);
        margin-bottom: 16px;
    }


		button {
			background-color: #4CAF50;
			color: white;
			padding: 8px;
			border: none;
			border-radius: 4px;
			cursor: pointer;
			font-size: 16px;
		}

		button:hover {
			background-color: #3e8e41;
		}
	</style>

	<script>
		function confirmOrder(coffeeName, coffeePrice) {
			var orderConfirmed = confirm("Are you sure you want to order " + coffeeName + " for " + coffeePrice + " Indian Rupees?");
			if (orderConfirmed) {
				alert("Your order for " + coffeeName + " has been confirmed.");
			}
		}
	</script>

</asp:Content>

